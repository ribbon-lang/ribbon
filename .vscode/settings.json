{
    "cSpell.files": [
        "**.bb",
        "**.rml",
        "**.zig",
        "**.md",
        "**.json",
    ],
    "cSpell.ignorePaths": [
        ".zig-cache/**",
        "zig-out/**",
        "NOTES.md",
    ],
    "cSpell.words": [
        "callc",
        "nbsp"
    ],
    "nasm.outputFormat": "elf64",
    "nasm.extraFlags": [
        "-f elf64",
        "-I src/mod/x64",
        "-DDBG",
        "-DJUMP_TABLE rax",
        "-DJUMP_TABLE_LENGTH rax",
    ],
}